<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
    <package extends="struts-default" name="OrderElement" namespace="/OrderElement">
        <action class="org.xmdl.taslak.webapp.action.OrderElementAction" method="list" name="orderElements">
            <result name="input">/WEB-INF/pages/orderElementList.jsp</result>
            <result>/WEB-INF/pages/orderElementList.jsp</result>
        </action>
        <action class="org.xmdl.taslak.webapp.action.OrderElementAction" method="edit" name="editOrderElement">
            <result>/WEB-INF/pages/orderElementForm.jsp</result>
            <result name="error">/WEB-INF/pages/orderElementList.jsp</result>
        </action>
        <action class="org.xmdl.taslak.webapp.action.OrderElementAction" method="save" name="saveOrderElement">
            <result name="input">/WEB-INF/pages/orderElementForm.jsp</result>
            <result name="cancel" type="redirect">orderElements.do</result>
            <result name="delete" type="redirect">orderElements.do</result>
            <result name="success" type="redirect">orderElements.do</result>
        </action>
        <action class="org.xmdl.taslak.webapp.action.OrderElementAction" method="copy" name="copyOrderElement">
            <result name="success">/WEB-INF/pages/orderElementForm.jsp</result>
        </action>
        <action class="org.xmdl.taslak.webapp.action.OrderElementAction" method="list" name="ajaxList">
            <result name="success">/WEB-INF/pages/orderElementListJson.jsp</result>
        </action>
    </package>
</struts>
