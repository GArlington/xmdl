<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.xmdl"
      label="XMDL Universal Code Generator"
      version="0.6.0.20081112_beta1"
      provider-name="xmdl.org"
      plugin="org.xmdl.xmdl.ui">

   <description url="http://www.xmdl.org">
      XMDL Universal Code Generator. For more information see project page on http://www.xmdl.org/
   </description>

   <copyright url="http://xmdl.googlecode.com/svn/trunk/xmdl.lib/varfiles/NOTICE.txt">
      See project site for more information.
   </copyright>

   <license url="http://www.eclipse.org/legal/epl-v10.html">
      Licensed under Eclipse Public License 1.0. See site for more information.
   </license>

   <url>
      <discovery label="XMDL Home Page" url="http://www.xmdl.org"/>
      <discovery label="Project page on Google code" url="http://code.google.com/p/xmdl/"/>
   </url>

   <requires>
      <import plugin="org.apache.commons.logging"/>
      <import plugin="org.eclipse.xtext"/>
      <import plugin="org.eclipse.xtext.util"/>
      <import plugin="org.eclipse.emf.ecore"/>
      <import plugin="org.eclipse.emf.common"/>
      <import plugin="org.antlr.runtime"/>
      <import plugin="org.eclipse.xpand"/>
      <import plugin="org.eclipse.xtend"/>
      <import plugin="org.eclipse.emf.mwe.core"/>
      <import plugin="org.eclipse.emf.mwe.utils"/>
      <import plugin="org.eclipse.xtend.typesystem.emf"/>
      <import plugin="org.eclipse.jface.text"/>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.jdt.core"/>
      <import plugin="org.eclipse.xtext.log4j"/>
      <import plugin="com.ibm.icu"/>
      <import plugin="org.xmdl.wdl" version="1.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.xtext.ui.core"/>
      <import plugin="org.eclipse.xtext.ui.common"/>
      <import plugin="org.eclipse.xtext.log4j" version="1.2.15" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.editors" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui.ide" version="3.4.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.xmdl.wdl.generator" version="0.7.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.apache.commons.beanutils"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.apache.log4j"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.xmdl.wdl"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.xmdl.wdl.generator"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.xmdl.wdl.taslak"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.xmdl.wdl.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
