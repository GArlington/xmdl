<?xml version="1.0" encoding="UTF-8"?><beans xmlns="http://www.springframework.org/schema/beans" xmlns:aop="http://www.springframework.org/schema/aop" xmlns:tx="http://www.springframework.org/schema/tx" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" default-lazy-init="true" xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd             http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.0.xsd             http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.0.xsd">

    <bean class="org.openqa.selenium.firefox.FirefoxDriver" id="firefoxDriver"/>
    
    <bean class="org.openqa.selenium.htmlunit.HtmlUnitDriver" id="htmlUnitDriver"/>
    
    <bean class="org.openqa.selenium.support.events.EventFiringWebDriver" id="webDriver">
        <constructor-arg ref="firefoxDriver"/>
    </bean>

    <bean class="org.xmdl.ida.lib.web.selenium.utils.WebDriverLogger" id="webDriverLogger">
        <constructor-arg ref="webDriver"/>
    </bean>
    
</beans>
