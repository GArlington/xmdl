<%@ jet 
	package="chrome.xmdl.core.templates.ui.web.jsp.part.impl" 
	imports="
			chrome.xmdl.*
			chrome.xmdl.gen.util.*
			chrome.xmdl.core.util.*
			"
	class="FormFieldDateImpl"
	startTag="<$" endTag="$>" 
%><$
	XAttribute attrib = (XAttribute) argument;
	XClass xClass = attrib.getXClass();	
	ClassHelper helper = XMDLClassHelper.INSTANCE;
	String formName = UIHelper.formObject(xClass);
$><layout:date name="<$=formName$>Form" property="<$=helper.uncapSafeName(xClass)$>.<$=attrib.getName()$>" styleClass="<$=helper.uncapSafeName(attrib)$>"  
	key="<$=helper.getObjectName(xClass)$>.form.<$=attrib.getName()$>" editType="date" patternKey="date.format" />